{
    "source_to_dest": [
        {"src":"etc/PlatformSpecificAssets/EOS/*", "dest": "PlatformSpecificAssets~/EOS/", "recurse": true},
        {"src":"CHANGELOG.md",                     "dest": ""},
        {"src":"LICENSE.md",                       "dest": ""},
        {"src":"README.md",                        "dest": ""},
        {"src":"etc/PackageTemplate/package.json", "dest": ""},
        {"src":"etc/PackageTemplate/Editor.meta",  "dest": ""},
        {"src":"etc/PackageTemplate/Runtime.meta", "dest": ""},
        {"src":"docs/*",                           "dest" : "Documentation~/", "recurse": true},
        
        {"src":"Assets/EOS/*.png", "dest": "Runtime/EOS/"},

        {"src":"Assets/Plugins/*",                                   "dest": "Runtime/"},
        {"src":"Assets/Plugins/Source/Core/*",                       "dest": "Runtime/Core/"},
        {"src":"Assets/Plugins/Source/Core/Utility/*.cs",            "dest": "Runtime/Core/Utility/"},
        {"src":"Assets/Plugins/Source/Core/Utility/Extensions/*.cs", "dest": "Runtime/Core/Utility/Extensions/"},
        {"src":"Assets/Plugins/Source/Editor/*",                     "dest": "Editor/"},
        
        {"src":"Assets/Plugins/Windows/*",                         "dest": "Runtime/Windows/"},
        {"src":"Assets/Plugins/Windows/x64/*",                     "dest": "Runtime/Windows/x64/"},
        {"src":"Assets/Plugins/Windows/x86/*",                     "dest": "Runtime/Windows/x86/"},
        {"src":"Assets/Plugins/Windows/Core/*",                    "dest": "Runtime/Windows/Core/"},
        {"src":"Assets/Plugins/Windows/Editor/*",                  "dest": "Editor/Windows/"},

        {"src":"Assets/Plugins/Linux/*",                           "dest": "Runtime/Linux/"},
        {"src":"Assets/Plugins/Linux/Core/*",                      "dest": "Runtime/Linux/Core/"},
        {"src":"Assets/Plugins/Linux/Editor/*",                    "dest": "Editor/Linux/"},
        
        {"src":"Assets/Plugins/macOS/*.cs",                        "dest": "Runtime/macOS/"},
        {"src":"Assets/Plugins/macOS/Core/*",                      "dest": "Runtime/macOS/Core/"},
        {"src":"Assets/Plugins/macOS/Editor/*",                    "dest": "Editor/macOS/"},
        
        {"src":"Assets/Plugins/Android/gradleTemplate.properties", "dest": "PlatformSpecificAssets~/EOS/Android/"},
        {"src":"Assets/Plugins/Android/*",                         "dest": "Runtime/Android/"},
        {"src":"Assets/Plugins/Android/Core/*",                    "dest": "Runtime/Android/Core/"},
        {"src":"Assets/Plugins/Android/Editor/*",                  "dest": "Editor/Android/"},

        {"src":"Assets/Plugins/iOS/*",                             "dest": "Runtime/iOS/"},
        {"src":"Assets/Plugins/iOS/Core/*",                        "dest": "Runtime/iOS/Core/"},
        {"src":"Assets/Plugins/iOS/Editor/*",                      "dest": "Editor/iOS/"},
        {"src":"Assets/Plugins/iOS/EOSSDK.framework/EOSSDK",       "dest": "Runtime/iOS/EOSSDK.framework/"},
        {"src":"Assets/Plugins/iOS/EOSSDK.framework/*.plist",      "dest": "Runtime/iOS/EOSSDK.framework/"},
        
        {"ignore_regex":"Assets/Plugins/Windows/.*/steam_api\\.dll"},
        {"ignore_regex":"Assets/Plugins/Windows/.*/steam_api64\\.dll"},

        {"src":"Assets/Plugins/Standalone/Editor/*",                             "dest": "Editor/Standalone/"},
        {"src":"Assets/Plugins/Standalone/Editor/anticheat_integritytool.cfg",   "dest": "Editor/Standalone/"},

        {"src":"tools/bin/*", "dest": "bin~/", "recurse": true},

        {"src":"Assets/link.xml", "dest": "Editor/"},

        {"src":"Assets/Plugins/Source/EOS_SDK/*",                   "dest": "Runtime/EOS_SDK/", "recurse": true },
        
        {"src":"Assets/Resources/SourceCodePro.ttf",                "dest": "Samples~/Samples/StandardSamples/Materials/"},
        {"src":"Assets/Prefab/*.prefab",                            "dest": "Samples~/Samples/StandardSamples/Prefab/"},
        {"src":"Assets/Scripts/*",                                  "dest": "Samples~/Samples/StandardSamples/Scripts/"},    

        {"src":"Assets/Scripts/UI/Achievements/*.cs",               "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Achievements/"},
        {"src":"Assets/Scripts/UI/Common/*.cs",                     "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Common/"},
        {"src":"Assets/Scripts/UI/CustomInvites/*.cs",              "dest": "Samples~/Samples/StandardSamples/Scripts/UI/CustomInvites/"},
        {"src":"Assets/Scripts/UI/Friends/*.cs",                    "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Friends/"},
        {"src":"Assets/Scripts/UI/Leaderboard/*.cs",                "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Leaderboard/"},
        {"src":"Assets/Scripts/UI/Lobbies/*.cs",                    "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Lobbies/"},
        {"src":"Assets/Scripts/UI/Login/*.cs",                      "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Login/"},
        {"src":"Assets/Scripts/UI/MatchMaking/*.cs",                "dest": "Samples~/Samples/StandardSamples/Scripts/UI/MatchMaking/"},
        {"src":"Assets/Scripts/UI/Metrics/*.cs",                    "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Metrics/"},    
        {"src":"Assets/Scripts/UI/Peer2Peer/*.cs",                  "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Peer2Peer/"},
        {"src":"Assets/Scripts/UI/Peer2Peer/HighFrequency/*.cs",    "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Peer2Peer/HighFrequency/"},
        {"src":"Assets/Scripts/UI/PlayerReports/*.cs",              "dest": "Samples~/Samples/StandardSamples/Scripts/UI/PlayerReports/"},
        {"src":"Assets/Scripts/UI/Storage/*.cs",                    "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Storage/"},
        {"src":"Assets/Scripts/UI/Storage/Player/*.cs",             "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Storage/Player/"},
        {"src":"Assets/Scripts/UI/Storage/Title/*.cs",              "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Storage/Title/"},
        {"src":"Assets/Scripts/UI/Store/*.cs",                      "dest": "Samples~/Samples/StandardSamples/Scripts/UI/Store/"},

        {"src":"Assets/Scripts/Utility/*.cs",                       "dest": "Samples~/Samples/StandardSamples/Scripts/Utility/"},  
        {"src":"Assets/Scripts/Utility/Extensions/*.cs",            "dest": "Samples~/Samples/StandardSamples/Scripts/Utility/Extensions/"},

        {"src":"Assets/Scripts/Core/*",                             "dest": "Samples~/Samples/StandardSamples/Scripts/Core/"},
        {"src":"Assets/Scripts/Steam/*",                            "dest": "Samples~/Samples/StandardSamples/Scripts/Steam/"},
        {"src":"Assets/Scripts/Discord/*",                          "dest": "Samples~/Samples/StandardSamples/Scripts/Discord/"},
        {"src":"Assets/Scripts/Oculus/*",                           "dest": "Samples~/Samples/StandardSamples/Scripts/Oculus/"},
        {"src":"Assets/Scripts/OpenID/*",                           "dest": "Samples~/Samples/StandardSamples/Scripts/OpenID/"},
        {"src":"Assets/Scripts/OpenID/CloudFunction/*",             "dest": "Samples~/Samples/StandardSamples/Scripts/OpenID/CloudFunction/"},
        {"src":"Assets/Scripts/SignInWithApple/*",                  "dest": "Samples~/Samples/StandardSamples/Scripts/SignInWithApple/"},
        {"src":"Assets/Scripts/SignInWithApple/Editor/*",           "dest": "Samples~/Samples/StandardSamples/Scripts/SignInWithApple/Editor/"},

        {"src":"Assets/Scenes/StandardSamples/*.unity",             "dest": "Samples~/Samples/StandardSamples/Scenes/"},
        {"src":"Assets/Scenes/SampleScenes.asset",                  "dest": "Samples~/Samples/StandardSamples/Scenes/"},
        {"src":"Assets/Materials/StandardSamplesMaterials/*",       "dest": "Samples~/Samples/StandardSamples/Materials/"},

        {"src":"Assets/Scenes/P2PNetcodeSample/*.unity",            "dest": "Samples~/Samples/P2PNetcodeSample/Scenes/"},
        {"src":"Assets/Materials/NetcodeMaterials/*",               "dest": "Samples~/Samples/P2PNetcodeSample/Materials/"},
        {"src":"Assets/Scripts/Networking/*.cs",                    "dest": "Samples~/Samples/P2PNetcodeSample/Scripts/Networking/"},
        {"src":"Assets/Scripts/UI/P2PNetcode/*",                    "dest": "Samples~/Samples/P2PNetcodeSample/Scripts/UI/"},

        {"src":"Assets/Scenes/PerformanceStressTestSample/*.unity", "dest": "Samples~/Samples/PerformanceStressTestSample/Scenes/"},
        {"src":"Assets/Materials/PerformanceStressTestMaterials/*", "dest": "Samples~/Samples/PerformanceStressTestSample/Materials/"},
        {"src":"Assets/Scripts/PostProcessing/*",                   "dest": "Samples~/Samples/PerformanceStressTestSample/Scripts/PostProcessing/"},
        {"src":"Assets/Resources/PerformanceStressTest_Profiles/*", "dest": "Samples~/Samples/PerformanceStressTestSample/Profiles/"},

        {"src":"Assets/Readme/*.txt",                               "dest": "Samples~/Samples/StandardSamples/Readme/"}

    ],
    "blacklist": [
        "*.png"
    ]
}